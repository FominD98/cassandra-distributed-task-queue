<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="..\..\Projects\Common.vars" Condition="$(CommonVarsImported) == ''" />
  <Import Project="$(CatalogueRootDirectory)\MsBuild\DeployTargets\Targets\EmbeddedFront.targets"/>

  <PropertyGroup>
    <TargetFrontSolution>WebWorms</TargetFrontSolution>
    <FrontName>WebWorms</FrontName>
	<ResourcesPage>WebWorms</ResourcesPage>

    <VirtualStaticContentOutput></VirtualStaticContentOutput>
    <PhysicalStaticContentOutput>WebWorms\Web\Statics</PhysicalStaticContentOutput>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedFrontSolutionFile Include="WebWorms\WebWorms.sln"/>
    <SharedResources Include="Shared">
      <VirtualName>Shared/</VirtualName>
      <PhysicalPath>WebWorms\Web\Statics</PhysicalPath>
    </SharedResources>
    <SharedResources Include="SharedBlocks">
      <VirtualName>SharedBlocks/</VirtualName>
      <PhysicalPath>WebWorms\Web\Blocks</PhysicalPath>
    </SharedResources>
  </ItemGroup>

</Project>